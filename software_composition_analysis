pipeline {
  agent any
  stages {
stage('Dependency Check Report') {
    steps {
        dependencyCheck additionalArguments: ''' 
            -o "./" 
            -s "./"
            -f "ALL" 
            --prettyPrint''', odcInstallation: 'dependency-check-6.0.3'
        dependencyCheckPublisher pattern: 'dependency-check-report.xml'
    }    
   }
   }
  }
